{
    "name": "credential-extractor",
    "version": "0.0.1",
    "description": "Extractors for credentials",
    "main": "dist/index.js",
    "files": [
        "dist",
        "builds"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TextsHQ/credential-extractor.git"
    },
    "binary": {
        "module_name": "index",
        "host": "https://github.com/TextsHQ/credential-extractor/releases/download/",
        "remote_path": "v{version}",
        "module_path": "builds/napi-{napi_build_version}",
        "package_name": "credential-extractor-{platform}-{arch}-napi-v{napi_build_version}.tar.gz",
        "napi_versions": [6],
        "pkg_path": "."
    },
    "scripts": {
        "build": "tsc && cargo-cp-artifact -nc builds/napi-6/index.node -- cargo build --message-format=json-render-diagnostics",
        "install": "node-pre-gyp install --fallback-to-build=false",
        "package": "node-pre-gyp package",
        "upload-binary": "node-pre-gyp-github publish",
        "test": "jest"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "cargo-cp-artifact": "^0.1",
        "jest": "^27.0.4",
        "node-pre-gyp-github": "https://github.com/rumblefrog/node-pre-gyp-github.git",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "node-pre-gyp": "^0.17.0"
    }
}
